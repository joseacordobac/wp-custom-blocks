{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/sliders-block",
  "version": "0.1.0",
  "title": "Slider",
  "category": "widgets",
  "icon": "slides",
  "description": "Slider wordpress plugin",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "slideConfig": {
      "type": "object",
      "default": {
        "slidesPerView": 1
      }
    },
    "sliderId": {
      "type": "string",
      "default": ""
    },
    "arrowIcon": {
      "type": "object",
      "default": {
        "next": false,
        "prev": false
      }
    }
  },
  "textdomain": "sliders-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}